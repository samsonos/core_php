{
  "name": "samsonos/php_core",
  "description": "SamsonPHP framework core module",
  "keywords": [
    "php",
    "framework",
    "samsonos",
    "samsonphp"
  ],
  "authors": [
    {
      "name": "Vitaly Iegorov",
      "email": "egorov@samsonos.com"
    }
  ],
  "license": "Open Software License (OSL) v 3.0",
  "homepage": "http://samsonphp.com/",
  "autoload": {
    "classmap": ["src/"],
    "psr-0": {
      "samsonos\\core\\": "src/",
      "samson\\core\\": "src/"
    },
    "psr-4":{
      "samsonphp\\core\\": "src/"
    },
    "files": ["src/samson.php"]
  },
  "require": {
    "samsonframework/base": "1.*",
    "samsonframework/resource": "1.*",
    "samsonphp/event": "1.*",
    "samsonphp/composer": "1.*",
    "samsonphp/config": "1.*",
    "samsonos/php_url": "*",
    "filp/whoops": "2.*",
    "samsonframework/container": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "config":{
    "github-protocols":["https"]
  }
}
